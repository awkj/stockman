{
  "package": {
    "productName": "stockman",
    "version": "1.0.3"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/dollar-blank.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.awkj.stockman",
      "icon": [
        "icons/money-bag.png"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "https://xueqiu.com/*",
          "https://stock.xueqiu.com/*",
          "http://localhost:10100/*",
          "https://upsort.com/*"
        ]
      }
    },
    "windows": [
      {
        "title": "MenuBar",
        "label": "MenuBar",
        "width": 380,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "decorations": true,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}