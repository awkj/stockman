{
  "package": {
    "productName": "stockman",
    "version": "1.0.6"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:5173",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/stockman.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.awkj.stockman",
      "icon": [
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "https://xueqiu.com/*",
          "https://stock.xueqiu.com/*",
          "http://localhost:10100/*",
          "https://upsort.com/*"
        ]
      }
    },
    "windows": [
      {
        "title": "stockman",
        "width": 380,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "decorations": false,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}